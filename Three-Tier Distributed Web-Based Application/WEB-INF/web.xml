<servlet>
      <servlet-name>SQLQueryClientServlet</servlet-name>
      <servlet-class>
        SQLQueryClientServlet
      </servlet-class>
   </servlet>
   <servlet-mapping>
    <servlet-name>SQLQueryClientServlet</servlet-name>
    <url-pattern>/SQLQueryClientServlet</url-pattern>
  </servlet-mapping>
</web-app>

